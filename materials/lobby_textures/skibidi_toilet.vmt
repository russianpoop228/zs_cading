"LightmappedGeneric"
{
    "$basetexture" "lobby_textures\skibidi_toilet"
    "$surfaceprop" "metal"
    "$translucent" "1"

    Proxies
    {
        AnimatedTexture
        {
            animatedtexturevar "$basetexture"
            animatedtextureframenumvar "$frame"
            animatedtextureframerate 10
        }
    }
}