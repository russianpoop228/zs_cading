"LightmappedGeneric"
{
    "$basetexture" "lobby_textures\fierce_uzs_ad"
    "$surfaceprop" "metal"
    "$translucent" "1"

    Proxies
    {
        AnimatedTexture
        {
            animatedtexturevar "$basetexture"
            animatedtextureframenumvar "$frame"
            animatedtextureframerate 5
        }
    }
}