"LightmappedGeneric"
{
    "$basetexture" "lobby_textures\meat"
    "$surfaceprop" "flesh"
    "$translucent" "1"

    Proxies
    {
        AnimatedTexture
        {
            animatedtexturevar "$basetexture"
            animatedtextureframenumvar "$frame"
            animatedtextureframerate 30
        }
    }
}